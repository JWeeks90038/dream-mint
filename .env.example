# DreamMint Solana Environment Configuration
# Copy this to .env and fill in your actual values

# OpenAI API Key (for AI image generation)
OPENAI_API_KEY=your-openai-api-key-here

# IPFS Storage (choose one)
VITE_PINATA_API_KEY=your-pinata-api-key-here
VITE_PINATA_SECRET_KEY=your-pinata-secret-key-here

# Solana Configuration
VITE_SOLANA_RPC_URL=https://api.mainnet-beta.solana.com
VITE_SOLANA_DEVNET_RPC_URL=https://api.devnet.solana.com
VITE_SOLANA_PROGRAM_ID=your-solana-program-id-here
VITE_SOLANA_DEVNET_PROGRAM_ID=your-devnet-program-id-here
VITE_PAYMENT_RECEIVER_PUBKEY=your-solana-wallet-address-here

# DreamMint Pricing Configuration (in cents)
VITE_IMAGE_GENERATION_PRICE=99
VITE_NFT_MINTING_PRICE=299
VITE_TOTAL_DREAM_PRICE=398

# Stripe Payment Processing
VITE_STRIPE_PUBLISHABLE_KEY=pk_test_your-stripe-publishable-key-here
STRIPE_SECRET_KEY=sk_test_your-stripe-secret-key-here

# Environment
NODE_ENV=development
